		"eslint": "^8.12.0",
		"eslint-config-next": "^12.1.2",
		"eslint-import-resolver-typescript": "^2.7.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-react": "^7.29.4",
		"eslint-plugin-react-hooks": "^4.4.0",
		"eslint-plugin-tailwindcss": "^3.5.0",
		"fast-glob": "^3.2.11",
		"react": "^17.0.2",
