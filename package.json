    "eslint-plugin-brackets": ">= 0.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "ethers": "5.7.2",
    "eventemitter3": "^5.0.1",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "husky": "^8.0.3",
