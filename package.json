{
	"name": "ydocumentation",
	"version": "0.1.0",
	"private": false,
	"scripts": {
		"dev": "next",
		"dev:ts": "tsc --watch",
		"start": "tsc && next build && next start",
		"install": "(cd lib && yarn install)",
		"build": "tsc && next build",
		"export": "tsc && next build && next export",
		"analyze": "cross-env ANALYZE=true next build",
		"lint": "eslint . --ext .js,.jsx,.ts,.tsx --ignore-pattern lib",
		"lintfix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
		"publish-patch": "cd lib && yarn publish --patch --access public --message",
		"publish-minor": "cd lib && yarn publish --minor --access public --message"
	},
	"dependencies": {
		"@dnd-kit/core": "^5.0.3",
		"@dnd-kit/sortable": "^6.0.1",
		"@dnd-kit/utilities": "^3.1.0",
		"@headlessui/react": "^1.5.0",
		"@linear/sdk": "^1.22.0",
		"@tailwindcss/forms": "^0.5.0",
		"@tailwindcss/line-clamp": "^0.3.1",
		"@tailwindcss/typography": "^0.5.2",
		"autoprefixer": "^10.4.4",
		"axios": "^0.26.1",
		"babel-loader": "^8.2.4",
		"ethcall": "^4.6.0",
		"ethers": "^5.6.4",
		"next": "^12.1.5",
		"next-seo": "^5.4.0",
		"prism-react-renderer": "^1.3.1",
		"prismjs": "^1.27.0",
		"react": "^18.0.0",
		"react-beautiful-dnd": "^13.1.0",
		"react-dom": "^18.0.0",
		"react-prism": "^4.3.2",
		"react-table": "^7.7.0",
		"sass": "^1.50.0",
		"sharp": "^0.30.3",
		"tailwindcss": "^3.0.24"
	},
	"devDependencies": {
		"@types/node": "^17.0.24",
		"@types/prismjs": "^1.26.0",
		"@types/react": "^18.0.5",
		"@types/react-dom": "^18.0.1",
		"@typescript-eslint/eslint-plugin": "^5.19.0",
		"@typescript-eslint/parser": "^5.19.0",
		"bufferutil": "^4.0.6",
		"dotenv-webpack": "^7.1.0",
		"encoding": "^0.1.13",
		"eslint": "^8.13.0",
		"eslint-config-next": "^12.1.5",
		"eslint-import-resolver-typescript": "^2.7.1",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-react": "^7.29.4",
		"eslint-plugin-react-hooks": "^4.4.0",
		"eslint-plugin-tailwindcss": "^3.5.0",
		"postcss": "^8.4.12",
		"postcss-import": "^14.1.0",
		"postcss-nesting": "^10.1.4",
		"ts-loader": "^9.2.8",
		"typescript": "^4.6.3",
		"utf-8-validate": "^5.0.9",
		"ws": "^8.5.0"
	}
}
