{
	"name": "ydocumentation",
	"version": "0.1.0",
	"private": false,
	"scripts": {
		"dev": "next",
		"dev:ts": "tsc --watch",
		"start": "tsc && next build && next start",
		"install": "(cd lib && yarn install)",
		"build": "tsc && next build",
		"export": "tsc && next build && next export",
		"analyze": "cross-env ANALYZE=true next build",
		"lint": "eslint . --ext .js,.jsx,.ts,.tsx --ignore-pattern lib",
		"lintfix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
		"publish-patch": "cd lib && rm -rf dist && yarn publish --patch --access public --message",
		"publish-minor": "cd lib && rm -rf dist && yarn publish --minor --access public --message"
	},
	"dependencies": {
		"@coinbase/wallet-sdk": "^3.1.0",
		"@dnd-kit/core": "^6.0.2",
		"@dnd-kit/sortable": "^7.0.0",
		"@dnd-kit/utilities": "^3.2.0",
		"@headlessui/react": "^1.6.3",
		"@tailwindcss/forms": "^0.5.2",
		"@tailwindcss/line-clamp": "^0.4.0",
		"@tailwindcss/typography": "^0.5.2",
		"@walletconnect/ethereum-provider": "^1.7.8",
		"@web3-react/coinbase-wallet": "^8.0.31-beta.0",
		"autoprefixer": "^10.4.7",
		"axios": "^0.27.2",
		"babel-loader": "^8.2.5",
		"ethcall": "^4.7.0",
		"ethers": "^5.6.8",
		"framer-motion": "^6.3.4",
		"next": "^12.1.6",
		"next-seo": "^5.4.0",
		"prism-react-renderer": "^1.3.3",
		"prismjs": "^1.28.0",
		"react": "^18.1.0",
		"react-beautiful-dnd": "^13.1.0",
		"react-dom": "^18.1.0",
		"react-prism": "^4.3.2",
		"react-table": "^7.8.0",
		"sass": "^1.52.1",
		"sharp": "^0.30.5",
		"tailwindcss": "^3.0.24"
	},
	"devDependencies": {
		"@types/node": "^17.0.35",
		"@types/prismjs": "^1.26.0",
		"@types/react": "^18.0.9",
		"@types/react-dom": "^18.0.5",
		"@typescript-eslint/eslint-plugin": "^5.26.0",
		"@typescript-eslint/parser": "^5.26.0",
		"bufferutil": "^4.0.6",
		"dotenv-webpack": "^7.1.0",
		"encoding": "^0.1.13",
		"eslint": "^8.16.0",
		"eslint-config-next": "^12.1.6",
		"eslint-import-resolver-typescript": "^2.7.1",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-react": "^7.30.0",
		"eslint-plugin-react-hooks": "^4.5.0",
		"eslint-plugin-tailwindcss": "^3.5.0",
		"postcss": "^8.4.14",
		"postcss-import": "^14.1.0",
		"postcss-nesting": "^10.1.7",
		"ts-loader": "^9.3.0",
		"typescript": "^4.7.2",
		"utf-8-validate": "^5.0.9",
		"ws": "^8.7.0"
	}
}
